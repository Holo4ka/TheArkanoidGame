<Window x:Class="TheArkanoidGame1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheArkanoidGame1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" x:Name="wdw">
    <Grid>
        <Canvas Name="canvas" Background="LightGray" Width="800" Height="450">
            <Ellipse Name="ball" Width="50" Height="50" Fill="Red" Canvas.Left="350" Canvas.Top="350"/>
            <Rectangle Name="rectangle" Width="100" Height="50" Fill="Black" Canvas.Left="35" Canvas.Top="24"/>
        </Canvas>
    </Grid>
</Window>
